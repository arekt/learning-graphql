type User {
  name: String
  projects: [Project]
}

type Project {
  name: String
  priority: Int
}

type Query {
  users: [User]
}

input UserInput {
  name: String
  projects: [ProjectInput]
}

input ProjectInput {
  name: String
  priority: Int
}

type Mutation {
  createUser(input:UserInput): User
}

